#!/bin/bash
################################################
#[仕様]
#・二つの自然数を引数にする
#　-> 引数のチェックは行わない
#・最大公約数のみを出力する
#
################################################

if [ $1 -gt $2 ] ; then
  ARG1=$1
  ARG2=$2
else
  ARG1=$2
  ARG2=$1
fi

while [ ${ARG2} -ne 0 ]
do
  TMP=$((${ARG1}%${ARG2}))
  ARG1=${ARG2}
  ARG2=$TMP
done

echo ${ARG1}

